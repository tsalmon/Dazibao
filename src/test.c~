/*
 *Ce fichier sert a tester l'interface graphique en simulant le resultat d'un dazibao
 */
#include <gtk/gtk.h>
#include <unistd.h>
#include <stdlib.h>
#include <stdio.h>
#include "test.h"
#include "vue.h"

//struct dazibao *daz;

void test(){
  daz = malloc(sizeof(dazibao));  
  
  //initialisation memoire
  daz->nb_tlv = 3;
  daz->tlv = malloc(sizeof(tlv_list) * 3);
  //il y a maintenant 3 tlv
  
  daz->tlv[0].type_id = 2; // 2 = text
  daz->tlv[1].type_id = 3; // 2 = text

  //on libre la memoire de la struct dazibao
  free(daz->tlv);
  free(daz);
  
}

int main(int argc, char *argv[]){
  test();
  //init(argc, argv);
  return 0;
}
